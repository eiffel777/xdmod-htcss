{
    "table_definition": {
        "name": "htcss_raw_logs",
        "engine": "InnoDB",
        "comment": "Summarized HTCondor Jobs",
        "columns": [
            {
                "name": "start_date",
                "type": "date",
                "default": "0000-00-00",
                "nullable": false
            },
            {
                "name": "end_date",
                "type": "date",
                "default": "0000-00-00",
                "nullable": false
            },
            {
                "name": "start_day_id",
                "type": "int(11)",
                "default": -1,
                "nullable": true
            },
            {
                "name": "end_day_id",
                "type": "int(11)",
                "default": -1,
                "nullable": true
            },
            {
                "name": "record_id",
                "type": "int(11)",
                "extra": "auto_increment",
                "nullable": false
            },
            {
                "name": "resource",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "queue",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "project",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "system_account",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "processor_count",
                "type": "int(11)",
                "nullable": true,
                "comment": "Instance identifier from cloud provider"
            },
            {
                "name": "gpu_count",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "wallduration",
                "type": "decimal(16,4)",
                "nullable": true
            },
            {
                "name": "running_job_count",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "waitduration",
                "type": "decimal(16,4)",
                "nullable": true
            },
            {
                "name": "submitted_job_count",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "ended_job_count",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "started_job_count",
                "type": "int(11)",
                "nullable": true
            },
            {
                "name": "date",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "resource_institution",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "field_of_science",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "project_pi",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "project_pi_institution",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "person",
                "type": "varchar(256)",
                "nullable": false
            },
            {
                "name": "person_institution",
                "type": "varchar(256)",
                "nullable": false
            }
        ],
        "indexes": [
            {
                "name": "PRIMARY",
                "columns": [
                    "record_id"
                ],
                "is_unique": true
            }
        ]
    }
}
